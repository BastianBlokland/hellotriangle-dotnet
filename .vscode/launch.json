{
   "version": "0.2.0",
   "configurations": [
        {
            "name": "macos",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build-macos-debug",
            "program": "${workspaceFolder}/platforms/ht.macos/bin/Debug/netcoreapp2.1/ht.macos.dll",
            "args": [],
            "cwd": "${workspaceFolder}/platforms/ht.macos",
            "console": "internalConsole",
            "stopAtEntry": false,
            "internalConsoleOptions": "openOnSessionStart"
        },
        {
            "name": "win32",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build-win32-debug",
            "program": "${workspaceFolder}/platforms/ht.win32/bin/Debug/netcoreapp2.1/ht.win32.dll",
            "args": [],
            "cwd": "${workspaceFolder}/platforms/ht.win32",
            "console": "internalConsole",
            "stopAtEntry": false,
            "internalConsoleOptions": "openOnSessionStart"
        },
        {
            "name": "attach",
            "type": "coreclr",
            "request": "attach",
            "processId": "${command:pickProcess}"
        }
    ,]
}